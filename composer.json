{
  "name": "wp-core-bootstrap/documentation-parser",
  "description": "Parse the WordPress code to generate documentation for the Core Bootstrap project.",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Alain Schlesser",
      "email": "alain.schlesser@gmail.com"
    }
  ],
  "require": {
    "johnpbloch/wordpress-core": "dev-master",
    "consolidation/robo": "^1.1",
    "nikic/php-parser": "^3.1",
    "symfony/filesystem": "^3.3",
    "symfony/finder": "^3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~6"
  },
  "autoload": {
    "psr-4": {
      "WPCoreBootstrap\\DocumentationParser\\": "src/"
    }
  }
}
